[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1

[libraries]
linux.debug.x86_64 = "res://../rust/target/debug/libgliding_platformer.so"
linux.release.x86_64 = "res://../rust/target/release/libgliding_platformer.so"
windows.debug.x86_64 = "res://../rust/target/debug/hello_world.dll"
windows.release.x86_64 = "res://../rust/target/release/hello_world.dll"
macos.debug = "res://../rust/target/debug/libgliding_platformer.dylib"
macos.release = "res://../rust/target/release/libgliding_platformer.dylib"
macos.debug.arm64 = "res://../rust/target/debug/libgliding_platformer.dylib"
macos.release.arm64 = "res://../rust/target/release/libgliding_platformer.dylib"
